{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/assets/*"
  ],
  "routes": [
    {
      "src": "^/result/.*$",
      "dest": "/index.html"
    },
    {
      "src": "^/styles\\.css$",
      "dest": "/styles.css"
    },
    {
      "src": "^/script\\.js$",
      "dest": "/script.js"
    }
  ]
}