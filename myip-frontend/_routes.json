{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [],
  "routes": [
    {
      "src": "^/result/.*$",
      "dest": "/index.html"
    },
    {
      "src": "^/styles\\.css$",
      "dest": "/styles.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "^/script\\.js$",
      "dest": "/script.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ]
}